#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	string s;
	cin>>s;
	int l;
	l = s.length();
	sort(s.begin(), s.end());
	int count=0;
	for(int i=0; i<l; i++){
	    if (s[i]!=s[i+1]){
	        count++;
	    }
	}
	if(count%2==0){
	    cout<<"CHAT WITH HER!"<<'\n';
	}else{
	    cout<<"IGNORE HIM!"<<'\n';
	}
	return 0;
}
